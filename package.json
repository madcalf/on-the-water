{
  "name": "on-the-water",
  "version": "2.0.0",
  "description": "Trip planning tool using NOAA Tides and Current APIs",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "start": "node server/app.js",
    "build:dev": "npm run build -- --watch --mode=development",
    "start:dev": "npm run build:dev & nodemon server/app.js & npm run sass",
    "start:dev:logger": "LOGGING=true npm run start:dev",
    "sass": "sass client:public/css --watch --no-source-map",
    "test": "jest --verbose",
    "test:dev": "npm run test -- --watch"
  },
  "author": "D",
  "license": "MIT",
  "engines": {
    "node": "16.10.0",
    "npm": "7.24.0"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ],
    "modulePaths": [
      "/shared/vendor/modules"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/pickers": "^3.3.10",
    "axios": "^0.24.0",
    "bootstrap": "^5.1.3",
    "chalk": "^4.1.2",
    "compression": "^1.7.3",
    "d3-scale": "^3.3.0",
    "express": "^4.16.4",
    "history": "^4.9.0",
    "http-errors": "^1.8.0",
    "leaflet": "^1.7.1",
    "morgan": "^1.9.1",
    "node-sass": "^8.0.0",
    "pg": "^8.5.1",
    "prop-types": "^15.7.2",
    "react-bootstrap": "^2.0.1",
    "react-compound-slider": "^3.3.1",
    "react-leaflet": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.12.10",
    "@babel/register": "^7.12.10",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "axios-mock-adapter": "^1.16.0",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.5.0",
    "eslint-plugin-jest-dom": "^3.9.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^5.0.0",
    "esm": "^3.2.25",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.3.1",
    "jest-transform-stub": "^2.0.0",
    "nodemon": "^1.19.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.0.1",
    "react-router-dom": "^5.0.0",
    "react-test-renderer": "^16.4.2",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "sass": "^1.43.4",
    "sass-loader": "^12.3.0",
    "style-loader": "^3.3.1",
    "svg-url-loader": "^7.1.1",
    "webpack": "^5.61.0",
    "webpack-cli": "^4.3.1"
  }
}
